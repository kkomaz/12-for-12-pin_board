#pins.transitions-enabled
  - @pins.each do |pin|
    .box.panel.panel-default
      %h2= link_to pin.title, pin
      .panel-body
        = link_to (image_tag pin.image.url), pin
        %p.user
        Submitted by
        = pin.user.email

.col-md-3.col-md-offset-5
  = will_paginate @pins, :previous_label => h("<"), :next_label => h(">")

